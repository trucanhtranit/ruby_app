.table-responsive
  %table.table.table-striped.table-hover#dataTable{width: "100%", cellspacing: "0"}
    %thead.table-dark
      %tr
        %th ID
        %th Email
        %th Actions
    %tbody
      - @admin_users.each do |admin_user|
        %tr
          %td= admin_user.id
          %td= admin_user.email
          %td.d-flex.gap-2
            = link_to edit_admin_admin_user_path(admin_user) do
              %i.fas.fa-edit
            = link_to admin_admin_user_path(admin_user), method: :delete, data: { confirm: 'Bạn có chắc chắn không?' } do
              %i.fas.fa-trash.text-danger
